{
  "is_production": false,
  "dev": {
    "log_filemode": "w",
    "traces_sample_rate": 1,
    "logger": "console"
  },
  "prod": {
    "log_filemode": "a",
    "traces_sample_rate": 0.8,
    "logger": "file"
  },
  "common": {
    "locale": "ru",
    "log_format": "%(asctime)s %(levelname)s %(message)s",
    "log_file": "bot_log.log"
  }
}
